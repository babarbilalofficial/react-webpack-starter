{
  "name": "react-webpack-starter",
  "version": "1.0.0",
  "description": "Boilerplate for react webpack",
  "main": "app.js",
  "scripts": {
    "start": "npm run start:dev",
    "build": "npm run build:prod",
    "start:dev": "webpack-dev-server --config webpack.dev.config.js",
    "build:prod": "cross-env NODE_ENV=production && webpack --config webpack.production.config.js -p --progress",
    "serve:prod": "cross-env NODE_ENV=production && node server/index.js"
  },
  "author": "Babar Bilal <babarxm2@gmail.com>",
  "license": "ISC",
