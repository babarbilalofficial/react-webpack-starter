    "babel-preset-react-app": "3.1.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "classnames": "2.2.6",
    "core-js": "2.5.5",
    "lodash": "4.17.19",
    "node-sass": "4.9.0",
